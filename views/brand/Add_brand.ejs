    <section class="content-main">
        <div class="content-header">
            <h2 class="content-title">Add Brand </h2>
        </div>
        <div class="card">
            <div class="card-body">
                <div class="row gx-5">
                    <div class="col-lg-9">
                        <section class="content-body p-xl-4">
                            <form action="/brand/insert_brand" method="post">
                                <div class="row">
                                    <div class="col-lg-8">
                                        <div class="d-flex align-items-center mb-3">
                                            <label class="form-label col-lg-4">Select category</label>
                                            <select class="form-select" name="categoryId" id="catId"
                                                onchange="return getsubcatData()">
                                                <option>-- Select category --</option>
                                                <% catData.map((v,i)=>{ %>
                                                    <option value="<%= v.id %>">
                                                        <%= v.category_name %>
                                                    </option>
                                                    <% }) %>
                                            </select>
                                        </div>
                                        <div class="d-flex align-items-center mb-3">
                                            <label class="form-label col-lg-4">Select Subcategory</label>
                                            <select class="form-select" name="subcategoryId" id="subcatId"
                                                onchange="return getextracatData()">
                                                <option>-- Select Subcategory --</option>
                                                <% subcatData.map((v,i)=>{ %>
                                                    <option value="<%= v.id %>">
                                                        <%= v.subcategory_name %>
                                                    </option>
                                                    <% }) %>
                                            </select>
                                        </div>
                                        <div class="d-flex align-items-center mb-3">
                                            <label class="form-label col-lg-4">Select Extracategory</label>
                                            <select class="form-select" name="extracategoryId" id="extracatId"
                                                onchange="return getBrandTypeData()">
                                                <option>-- Select Extracategory --</option>
                                                <% extracatData.map((v,i)=>{ %>
                                                    <option value="<%= v.id %>">
                                                        <%= v.extracategory_name %>
                                                    </option>
                                                    <% }) %>
                                            </select>
                                        </div>
                                        <div class="d-flex gx-4 align-items-center">
                                            <label class="form-label col-lg-4">Add Brand</label>
                                            <input type="text" placeholder="brand name" class="form-control"
                                                name="brand_name">
                                        </div>
                                    </div>
                                </div>
                                <br>
                                <button class="btn btn-primary" type="submit">Add Brand</button>
                            </form>
                        </section>
                    </div>
                </div>
            </div>
        </div>
    </section>

