<div class="container-fluid pt-5">
    <div class="row px-xl-5 pb-3">
        <% Product.map((item,i)=>{%>
            <div class="col-lg-4 col-md-6 pb-1">
                <a href="/singleproductpage/<%=item.id%>" class="cat-img position-relative overflow-hidden mb-3">
                    <div class="cat-item d-flex flex-column border mb-4" style="padding: 30px;">
                        <img class="img-fluid" src="<%=item.images%>" alt="">
                        <h5 class="font-weight-semi-bold m-0">
                            <%=item.ProductName%>
                        </h5>
                        <div class="d-flex">
                            <h5 class="text-danger font-weight-medium m-0">
                                <%=item.Price%>
                            </h5>
                            <h5 class="font-weight-semi-bold m-0">
                                <%=item.SellingPrice%>
                            </h5>
                        </div>
                    </div>
                </a>
            </div>
            <%}) %>
    </div>
</div>