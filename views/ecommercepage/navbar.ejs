<div class="container-fluid mb-5">
    <div class="row border-top px-xl-5">
        <div class="col-lg-3 ">
            <div class="widget-category mb-30">
                <h5 class=" style-1 mb-30 wow fadeIn animated">Sub Category</h5>
                <ul class="categories">
                    <% for (var scd of subcatData) { %>
                        <li><a href="/">
                                <%= scd.subcategory_name %>
                            </a></li>
                        <% } %>
                </ul>
            </div>
            <div class="widget-category mb-30 ">
                <h5 class=" style-1 mb-30 ">Sort by</h5>
                <ul class="categories">
                    <li><a href="/">Popularity</a></li>
                    <li><a href="/">Average rating</a></li>
                    <li><a href="/">Price: Low to High</a></li>
                    <li><a href="/">Price: High to Low</a></li>
                </ul>
            </div>

        </div>
        <div class="col-lg-9">
            <nav class="navbar navbar-expand-lg bg-light navbar-light py-3 py-lg-0 px-0">
                <a href="" class="text-decoration-none d-block d-lg-none">
                    <h1 class="m-0 display-5 font-weight-semi-bold"><span
                            class="text-primary font-weight-bold border px-3 mr-1">E</span>Shopper</h1>
                </a>
                <button type="button" class="navbar-toggler" data-toggle="collapse" data-target="#navbarCollapse">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse justify-content-between" id="navbarCollapse">
                    <ul>
                        <% for (var cd of catData){ %>
                            <li class="position-static"><a href="#">
                                    <%= cd.category_name %>
                                </a>
                                <ul class="mega-menu">
                                    <% for (var scd of subcatData){ %>
                                        <% if(cd.id==scd.categoryId) {%>
                                            <li class="sub-mega-menu sub-mega-menu-width-22">
                                                <a class="menu-title" href="#">
                                                    <%= scd.subcategory_name %>
                                                </a>
                                                <ul>
                                                    <% for (var ecd of extracatData){ %>
                                                        <% if(scd.id==ecd.subcategoryId) {%>
                                                            <li><a href="/"></a>
                                                                <%= ecd.extracategory_name %>
                                                                    </a>
                                                            </li>
                                                            <% } %>
                                                                <% } %>
                                                </ul>
                                            </li>
                                            <% } %>
                                                <% } %>
                                </ul>
                            </li>
                            <% } %>
                    </ul>
                    <div class="navbar-nav ml-auto py-0">
                       
                        <% (locals.user) %>
                            <% if(locals.user){ %>
                               <a href="#">
                                        Hello &nbsp;<%= locals.user.username %>
                                    </a>
                               <a class="text-danger"
                                        href="/userlogout">&nbsp;&nbsp;Logout</a>
                                <% }else{ %>
                                    <a href="/usertlogin">user
                                            Log In</a>
                                    <% } %>
                                        <a href="/usersignup" class="nav-item nav-link">Register</a>
                                        <a href="/login" class="nav-item nav-link"> admin login</a>

                    </div>
                    <div class="header-action-icon-2">
                        <a href="shop-wishlist.html">
                            <img src="/ecommerce//img/heart-regular.svg" alt="">
                            <span class="pro-count blue">0</span>
                        </a>
                    </div>
                    <div class="header-action-icon-2">
                        <a class="mini-cart-icon" href="<% (locals.user) %>
                            <% if(locals.user){ %>
                            <%= `/checkout` %>
                           <% }else{ %>
                            <%= `/userlogin` %>
                            <% } %>"">
                            <img src="/ecommerce/img/bag-shopping-solid.svg">
                            <span class="pro-count blue"><%= totalPro %></span>
                        </a>
                    </div>
                </div>
            </nav>
            <div id="header-carousel" class="carousel slide" data-ride="carousel">
                <div class="carousel-inner">
                    <div class="carousel-item active" style="height: 410px;">
                        <img class="img-fluid" src="img/carousel-1.jpg" alt="Image">
                        <div class="carousel-caption d-flex flex-column align-items-center justify-content-center">
                            <div class="p-3" style="max-width: 700px;">
                                <h4 class="text-light text-uppercase font-weight-medium mb-3">10% Off Your First Order
                                </h4>
                                <h3 class="display-4 text-white font-weight-semi-bold mb-4">Fashionable Dress</h3>
                                <a href="" class="btn btn-light py-2 px-3">Shop Now</a>
                            </div>
                        </div>
                    </div>
                    <div class="carousel-item" style="height: 410px;">
                        <img class="img-fluid" src="img/carousel-2.jpg" alt="Image">
                        <div class="carousel-caption d-flex flex-column align-items-center justify-content-center">
                            <div class="p-3" style="max-width: 700px;">
                                <h4 class="text-light text-uppercase font-weight-medium mb-3">10% Off Your First Order
                                </h4>
                                <h3 class="display-4 text-white font-weight-semi-bold mb-4">Reasonable Price</h3>
                                <a href="" class="btn btn-light py-2 px-3">Shop Now</a>
                            </div>
                        </div>
                    </div>
                </div>
                <a class="carousel-control-prev" href="#header-carousel" data-slide="prev">
                    <div class="btn btn-dark" style="width: 45px; height: 45px;">
                        <span class="carousel-control-prev-icon mb-n2"></span>
                    </div>
                </a>
                <a class="carousel-control-next" href="#header-carousel" data-slide="next">
                    <div class="btn btn-dark" style="width: 45px; height: 45px;">
                        <span class="carousel-control-next-icon mb-n2"></span>
                    </div>
                </a>
            </div>
        </div>
    </div>
</div>